  "sections": {
    "summoning": {
      "title": "Summoning Style",
      "description": "How do you bring your clones into the world?",
      "choices": {
        "at_will": {
          "title": "At Will",
          "description": "Just think of it, and they’ll pop into existence right next to you.",
          "cost": 1,
          "src": "https://i.imgur.com/T40SGNv.png",
          "requirements": {
            "exclude": null,
            "include": null
          }
        },
        "cumshot": {
          "title": "Cumshot",
          "description": "Whenever you orgasm that fluid will grow into a perfect copy of you. You get to choose which shot becomes a person.",
          "cost": 0,
          "src": "https://i.imgur.com/P1QQnSY.jpg",
          "requirements": {
            "exclude": null,
            "include": null
          }
        },
        "physical_trauma": {
          "title": "Physical Trauma",
          "description": "You have to get punched, slapped, or hit to spawn a clone. It has to be hard enough to hurt, but doesn’t need to break bones. The more painful the attack, the more clones it’ll create in one go.",
          "cost": 0,
          "src": "https://i.imgur.com/Og6GvAT.png",
          "requirements": {
            "exclude": null,
            "include": null
          }
        },
        "random": {
          "title": "Random",
          "description": "Clones just appear out of your body at random times. If you have a maximum number of clones, (say four clones) then the fifth clones will make the first desummon automatically.",
          "cost": 0,
          "src": "https://i.imgur.com/lWkpRX5.png",
          "requirements": {
            "exclude": null,
            "include": null
          }
        },
        "consumption": {
          "title": "Consumption",
          "description": "Whenever you eat a human’s worth of matter (About seventy kilos of food or water), a clone will be created. This is accumulated over your life time, and does not need to be in one sitting.",
          "cost": 0,
          "src": "https://i.imgur.com/835drrG.jpg",
          "requirements": {
            "exclude": null,
            "include": null
          }
        },
        "always_out": {
          "title": "Always Out",
          "description": "You don’t create your clones, they’re always there. You cannot take any desummon options, nor can you make more if those clones die.",
          "cost": 0,
          "src": "https://i.imgur.com/NL0MoDe.jpg",
          "requirements": {
            "exclude": null,
            "include": null
          }
        }
      }
    },
    "desummoning": {
      "title": "Desummoning Style",
      "description": "How do you get rid of the clones once you’ve made them?",
      "choices": {
        "anywhere": {
          "title": "Anywhere",
          "description": "Just think of the clone you want to get rid of, and they’ll disappear, leaving only the things they were holding/wearing.",
          "cost": 1,
          "src": "https://i.imgur.com/mkA57Km.png",
          "requirements": {
            "exclude": null,
            "include": null
          }
        },
        "touch": {
          "title": "Touch",
          "description": "Same as Anywhere, but you have to be touching the clone.",
          "cost": 0,
          "src": "https://i.imgur.com/p5dN2zZ.jpg",
          "requirements": {
            "exclude": null,
            "include": null
          }
        },
        "orgasm": {
          "title": "Orgasm",
          "description": "If the clone has an orgasm, it’ll cease to be.",
          "cost": 0,
          "src": "https://i.imgur.com/i0W5ljI.png",
          "requirements": {
            "exclude": null,
            "include": null
          }
        },
        "screaming_match": {
          "title": "Screaming Match",
          "description": "If the clone is defeated in a verbal argument, then it will cease to be. Defeated counts as retreating, giving up, accepting the other’s point, or saying something to the effect of 'agree to disagree'.",
          "cost": 0,
          "src": "https://i.imgur.com/QTEyalk.jpg",
          "requirements": {
            "exclude": null,
            "include": null
          }
        },
        "factory_reset": {
          "title": "Factory Reset",
          "description": "When you go to sleep, all your clones will be absorbed back into you.",
          "cost": 0,
          "src": "https://i.imgur.com/n2v2rV1.jpg",
          "requirements": {
            "exclude": null,
            "include": null
          }
        },
        "murder": {
          "title": "Murder",
          "description": "You have to kill the clone to desummon them. This is the only option for Always out.",
          "cost": -1,
          "src": "https://i.imgur.com/fsY4Psl.jpg",
          "requirements": {
            "exclude": null,
            "include": ["Always Out"]
          }
        }
      }
    },
