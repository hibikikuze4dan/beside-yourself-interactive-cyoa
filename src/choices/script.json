  "sections": {
    "summoning": {
      "title": "Summoning Style",
      "description": "How do you bring your clones into the world?",
      "choices": {
        "at_will": {
          "title": "At Will",
          "description": "Just think of it, and they’ll pop into existence right next to you.",
          "cost": 1,
          "src": "https://i.imgur.com/T40SGNv.png",
          "requirements": {
            "exclude": null,
            "include": null
          }
        },
        "cumshot": {
          "title": "Cumshot",
          "description": "Whenever you orgasm that fluid will grow into a perfect copy of you. You get to choose which shot becomes a person.",
          "cost": 0,
          "src": "https://i.imgur.com/P1QQnSY.jpg",
          "requirements": {
            "exclude": null,
            "include": null
          }
        },
        "physical_trauma": {
          "title": "Physical Trauma",
          "description": "You have to get punched, slapped, or hit to spawn a clone. It has to be hard enough to hurt, but doesn’t need to break bones. The more painful the attack, the more clones it’ll create in one go.",
          "cost": 0,
          "src": "https://i.imgur.com/Og6GvAT.png",
          "requirements": {
            "exclude": null,
            "include": null
          }
        },
        "random": {
          "title": "Random",
          "description": "Clones just appear out of your body at random times. If you have a maximum number of clones, (say four clones) then the fifth clones will make the first desummon automatically.",
          "cost": 0,
          "src": "https://i.imgur.com/lWkpRX5.png",
          "requirements": {
            "exclude": null,
            "include": null
          }
        },
        "consumption": {
          "title": "Consumption",
          "description": "Whenever you eat a human’s worth of matter (About seventy kilos of food or water), a clone will be created. This is accumulated over your life time, and does not need to be in one sitting.",
          "cost": 0,
          "src": "https://i.imgur.com/835drrG.jpg",
          "requirements": {
            "exclude": null,
            "include": null
          }
        },
        "always_out": {
          "title": "Always Out",
          "description": "You don’t create your clones, they’re always there. You cannot take any desummon options, nor can you make more if those clones die.",
          "cost": 0,
          "src": "https://i.imgur.com/NL0MoDe.jpg",
          "requirements": {
            "exclude": null,
            "include": null
          }
        }
      }
    },
